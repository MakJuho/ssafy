			int cnt=0;
			int pre=-1;
			int total = 0;
			for(int j=0; j<data.length(); j++) {
				total += Character.getNumericValue(data.charAt(j));
			}
			
				
			if(total !=0 ) {
				for(int k=data.length(); k>0; k--) {
					if(data.substring(k,k-1)!=data.substring(k-1,k-2))
					{
						
					}
				}
				
				
				
			}
			
			sb.append(cnt).append("\n");
			
			
			
			
		}
		
		
		/*for(int i=1; i<=TC; i++) {
